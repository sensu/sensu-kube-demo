{
    "type": "Handler",
    "spec": {
        "name": "influxdb",
        "type": "pipe",
        "runtime_assets": [
            "influxdb-handler"
        ],
        "command": "/usr/bin/sensu-influxdb-handler -a 'http://influxdb.default.svc.cluster.local:8086' -d sensu -u sensu -p password",
        "timeout": 10,
        "filters": [
            "has_metrics"
        ],
        "organization": "acme",
        "environment": "demo"
    }
}
